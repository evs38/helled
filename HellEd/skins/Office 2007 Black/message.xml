<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Author>Trooper</Author>
  <Name>FTN</Name>
  <Desc />
  <Copyright />
  <FileExtension />
  <FileType />
  <Version>1.1</Version>
  <Styles>
    <Style>
      <Name>Text</Name>
      <ForeColor>Black</ForeColor>
      <BackColor>White</BackColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>UUE</Name>
      <ForeColor>Blue</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>Tearline</Name>
      <ForeColor>Chocolate</ForeColor>
      <Index>2</Index>
    </Style>
    <Style>
      <Name>Origin</Name>
      <ForeColor>DarkViolet</ForeColor>
      <Index>3</Index>
    </Style>
    <Style>
      <Name>Kludge</Name>
      <ForeColor>Crimson</ForeColor>
      <Index>4</Index>
    </Style>
    <Style>
      <Name>OddQuote</Name>
      <ForeColor>ForestGreen</ForeColor>
      <Index>5</Index>
    </Style>
    <Style>
      <Name>EvenQuote</Name>
      <ForeColor>DarkBlue</ForeColor>
      <Index>6</Index>
    </Style>
    <Style>
      <Name>b</Name>
      <ForeColor>WindowText</ForeColor>
      <FontStyle>Bold</FontStyle>
      <Index>7</Index>
    </Style>
    <Style>
      <Name>i</Name>
      <ForeColor>WindowText</ForeColor>
      <FontStyle>Italic</FontStyle>
      <Index>8</Index>
    </Style>
    <Style>
      <Name>u</Name>
      <ForeColor>WindowText</ForeColor>
      <FontStyle>Underline</FontStyle>
      <Index>9</Index>
    </Style>
    <Style>
      <Name>h1</Name>
      <ForeColor>HighlightText</ForeColor>
      <BackColor>Highlight</BackColor>
      <Index>10</Index>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>Text</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
         <SyntaxBlock>
          <Name>UUE</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>^begin\s644\s</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>Tearline</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>^---\s.*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>Origin</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>^\s\*\sOrigin:\s.*\((?:\d*:)?\d*/\d*(?:\.\d*)?\)$</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>Kludge</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\u0001.*</string>
          </Expressions>
          <Index>3</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>OddQuote</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>^\s*[A-Za-z]*&gt;\s.*</string>
            <string>^\s*[A-Za-z]*(&gt;&gt;)*&gt;\s.*</string>
          </Expressions>
          <Index>4</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>EvenQuote</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>^\s*[A-Za-z]*&gt;&gt;*\s.*</string>
            <string>^\s*[A-Za-z]*(&gt;&gt;)*&gt;&gt;\s.*</string>
          </Expressions>
          <Index>5</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>0</Index>
    </State>
    <State>
      <Name>UUE</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>end</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>^end$</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>UUE</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>.*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
       </SyntaxBlocks>
      <Index>1</Index>
    </State>
  </States>
</LexScheme>